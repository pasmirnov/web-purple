{
  "name": "web-purple",
  "version": "1.2.0",
  "description": "Web Purple Community Web Site",
  "main": "src/boot.jsx",
  "engines": {
    "node": "6.2.0"
  },
  "scripts": {
    "start": "node src/server/server.js",
    "build": "webpack",
    "lint": "./node_modules/.bin/eslint src/** test/**",
    "test": "jest",
    "test:cov": "jest --coverage",
    "precoveralls": "npm run test:cov",
    "coveralls": "coveralls < coverage/lcov.info || true",
    "clean": "rimraf node_modules coverage __build__"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kitos/web-purple.git"
  },
  "author": "Nikita_Kirsanov@epam.com, Ilya_Kuznetsov@epam.com",
  "license": "ISC",
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-2",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "react-hot-loader/babel",
      "syntax-dynamic-import"
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs",
          "dynamic-import-node"
        ]
      },
      "travisci": {
        "plugins": [
          "transform-es2015-modules-commonjs",
          "dynamic-import-node"
        ]
      }
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testRegex": "test.(js|jsx)$",
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "cheerio": "^0.20.0",
    "color": "^1.0.3",
    "compression": "^1.6.2",
    "connect-history-api-fallback": "^1.2.0",
    "core-js": "^2.4.1",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "history": "^4.6.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash.unionwith": "^4.6.0",
    "moment": "^2.18.1",
    "mongoose": "^4.4.7",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.0",
    "passport-vkontakte": "^0.4.0",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.3.2",
    "react-datepicker": "^0.44.0",
    "react-dom": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-masonry-component": "^5.0.4",
    "react-modal": "^1.6.5",
    "react-redux": "^4.4.1",
    "react-router": "^4.1.1",
    "react-router-config": "^1.0.0-beta.3",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "react-select": "^1.0.0-rc.3",
    "redux": "^3.3.1",
    "redux-form": "^6.6.3",
    "redux-thunk": "^2.0.1",
    "reselect": "^3.0.0",
    "rss": "^1.2.1",
    "styled-components": "^1.4.3",
    "webpack": "^2.2.1",
    "webpack-hot-middleware": "^2.17.0"
  },
  "devDependencies": {
    "@types/jest": "^16.0.4",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.8.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-dynamic-import-node": "^1.0.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.11.6",
    "coveralls": "^2.11.12",
    "css-loader": "^0.28.0",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.3.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.0",
    "fetch-mock": "^5.5.0",
    "force-case-sensitivity-webpack-plugin": "^0.1.1",
    "html-webpack-plugin": "^2.22.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "jest-enzyme": "^3.0.0",
    "react-addons-test-utils": "^15.4.2",
    "redux-mock-store": "^1.2.1",
    "sinon": "^1.17.3",
    "style-loader": "^0.16.1",
    "webpack-dev-middleware": "^1.10.0"
  }
}
